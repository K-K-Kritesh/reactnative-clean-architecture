{
  "name": "sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "compile": "rm -rf dist && tsc",
    "android:clean": "cd android && ./gradlew clean",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:assembleDebug": "cd android && ./gradlew assembleDebug",
    "android:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && npm run compile && npm run android:clean && npm run android:assembleRelease",
    "android:debug": "npm run compile && npm run android:clean && npm run android:assembleDebug",
    "android:dev": "concurrently \"tsc -w\" \"npm run android:clean && react-native run-android\"",
    "android:devs": "concurrently \"tsc -w\" \"npm run android:clean && react-native run-android --deviceId=192.168.1.57:5555\"",
    "ios:dev": "concurrently \"tsc -w\" \"react-native run-ios\"",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@yoonzm/react-native-alphabet-flat-list": "^1.3.0",
    "axios": "^0.25.0",
    "concurrently": "^7.0.0",
    "formik": "^2.2.9",
    "ramda": "^0.28.0",
    "react": "17.0.2",
    "react-if": "^4.1.1",
    "react-native": "0.66.4",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-reanimated": "^2.3.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-table-component": "^1.2.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-view-shot": "^3.1.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "rn-progress-loader": "^1.0.8",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/ramda": "^0.27.64",
    "@types/react-native": "^0.66.4",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
